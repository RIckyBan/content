{
  "rules": {
    "preset-ja-spacing": {
      "ja-space-between-half-and-full-width": {
        "space": "always",
        "exceptPunctuation": false
      }
    },
    "editorconfig": {
      "charset": false
    },
    "preset-ja-technical-writing": {
      "ja-technical-writing/no-exclamation-question-mark": false,
      "ja-technical-writing/sentence-length": false
    }
  }
}
